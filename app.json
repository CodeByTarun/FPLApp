{
  "expo": {
    "name": "FPLApp",
    "slug": "FPLApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#451295"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.codebytarun.fplapp",
      "requireFullScreen": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#451295"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.codebytarun.fplapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "sdkVersion": "44.0.0"
  },
  "name": "fplapp"
}
